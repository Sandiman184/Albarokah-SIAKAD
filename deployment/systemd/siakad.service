[Unit]
Description=Gunicorn instance to serve SIAKAD App
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Albarokah/siakad_app
Environment="PATH=/home/ubuntu/Albarokah/venv/bin"
Environment="FLASK_APP=run.py"
ExecStart=/home/ubuntu/Albarokah/venv/bin/gunicorn --workers 3 --bind 127.0.0.1:8002 -m 007 run:app

[Install]
WantedBy=multi-user.target
